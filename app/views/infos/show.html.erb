<div class="booyah-box col-10 offset-1">
  <h1><%= @info.name %></h1>
  <br/><br/
  <i><%= @info.information %></i>
  <div class="float-right">
    <%= link_to 'Edit', edit_info_path(@info), class: 'btn btn-primary' %>
    <%= link_to 'Destroy', info_path(@info), method: :delete, data: {confirm: 'Are you sure you want to delete this?'},class: 'btn btn-danger' %>
  </div>
</div>